[core]
  excludesfile = /Users/nakanomizuki/.gitignore_global
  editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
  name = MizukiNakano
  email = mizuki590@gmail.com
[color]
  ui = auto
[alias]
  st = status
  lp = log -p
  cm = commit
  cmm = commit -m 
  amend = commit --amend
  co = checkout
  cb = checkout -b
  br = branch
  bd = branch -d
  mnff = merge --no-ff
  ps = push
  psom = push origin master
